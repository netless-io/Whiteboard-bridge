    "@netless/iframe-bridge": "2.1.5",
    "@netless/synced-store": "0.0.2",
    "@netless/video-js-plugin": "0.3.8",
    "@netless/white-audio-plugin": "^1.2.4",
    "@netless/white-audio-plugin2": "^2.0.4",
    "@netless/white-video-plugin": "^1.2.4",
    "@netless/white-video-plugin2": "^2.0.4",
    "@netless/window-manager": "0.4.4",
    "buffer": "^6.0.3",
    "core-js": "^3",
    "dsbridge": "^3.1.4",
    "html2canvas": "^1.3.3",
