{
  "name": "whiteboard-bridge",
  "version": "2.9.0",
  "main": "index.js",
  "private": true,
  "browserslist": [
    "Android 4.4",
    "iOS 9"
  ],
  "scripts": {
    "bundle": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --open --hot --host 0.0.0.0 --useLocalIp",
    "upgrade-plugin": "yarn upgrade --force @netless/white-audio-plugin @netless/white-video-plugin @netless/cursor-tool @netless/white-audio-plugin2 @netless/white-video-plugin2 @netless/video-js-plugin",
    "build": "tsc && webpack --mode production && touch ./build/$(git rev-parse --short HEAD) && git status -s >> ./build/$(git rev-parse --short HEAD)",
    "buildDev": "tsc && webpack --mode development && touch ./build/$(git rev-parse --short HEAD) && git status -s >> ./build/$(git rev-parse --short HEAD)",
    "check": "tsc"
  },
  "resolutions": {
    "**/video.js": "7.10.0"
  },
  "dependencies": {
    "@netless/combine-player": "^1.1.1",
    "@netless/cursor-tool": "^0.1.0",
    "@netless/iframe-bridge": "^2",
    "@netless/synced-store": "^0.0.1",
    "@netless/video-js-plugin": "0.3.4",
    "@netless/white-audio-plugin": "^1.2.4",
    "@netless/white-audio-plugin2": "^2.0.4",
    "@netless/white-video-plugin": "^1.2.4",
    "@netless/white-video-plugin2": "^2.0.4",
    "@netless/window-manager": "0.1.12",
    "core-js": "^3",
    "dsbridge": "^3.1.4",
    "html2canvas": "^1.0.0-rc.5",
    "promise.any": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.2.4",
    "whatwg-fetch": "^3.0.0",
    "white-web-sdk": "2.13.17"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/css-font-loading-module": "^0.0.4",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "tslint": "^5.20.1",
    "tslint-immutable": "^6.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
