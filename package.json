{
  "name": "whiteboard-bridge",
  "version": "2.15.0",
  "main": "index.js",
  "private": true,
  "browserslist": [
    "Android 4.4",
    "iOS 9"
  ],
  "scripts": {
    "bundle": "webpack",
    "start": "webpack-dev-server --mode development --host local-ip --hot --open",
    "build": "tsc && webpack --mode production && touch ./build/$(git rev-parse --short HEAD) && git status -s >> ./build/$(git rev-parse --short HEAD)",
    "buildDev": "tsc && webpack --mode development && touch ./build/$(git rev-parse --short HEAD) && git status -s >> ./build/$(git rev-parse --short HEAD)",
    "check": "tsc"
  },
  "resolutions": {
    "**/video.js": "7.10.0"
  },
  "dependencies": {
    "@netless/app-slide": "0.0.56",
    "@netless/combine-player": "^1.1.1",
